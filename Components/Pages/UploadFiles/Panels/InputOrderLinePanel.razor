@using LabManagement.Components.Pages.Controls
@implements IDialogContentComponent<CaseOrderLine>
@inject ISalesOrderResposity context
@inject IProductResposity productDB

@rendermode InteractiveServer
<FluentDialogHeader ShowDismiss="false">
    <FluentStack VerticalAlignment="VerticalAlignment.Center">
        <FluentIcon Value="@(new Icons.Regular.Size24.AppsListDetail())" />
        <FluentLabel Typo="Typography.PaneHeader">
            @Dialog.Instance.Parameters.Title
        </FluentLabel>
    </FluentStack>
</FluentDialogHeader>

<FluentStack Orientation="Orientation.Horizontal">

    <FluentStack Orientation="Orientation.Vertical">
        @if (@ErrorMessage != "")
        {
            <FluentLabel Typo="Typography.H5" Color="Color.Warning">@ErrorMessage</FluentLabel>
        }
        <FluentStack Orientation="Orientation.Horizontal" HorizontalAlignment="HorizontalAlignment.Start">
            <FluentLabel Style="width:100px">Select Product</FluentLabel>
            <FluentAutocomplete TOption="Product"
            AutoComplete="off"
            Width="300px"
            Disabled ="@(Content.RecID>0?true:false)"
            OnOptionsSearch="OnSearchProduct"
            Placeholder="Select Products"
            MaximumOptionsSearch="int.MaxValue"
            @bind-Value="Content.ItemID"
            MaximumSelectedOptions="1"
            Virtualize="true"
            OptionText="@(item => $"{item.ItemName}")"
            @bind-SelectedOptions="@SelectedProducts" />
        </FluentStack>

        @if (Content.RecID > 0)
        {
            <FluentStack Orientation="Orientation.Horizontal" HorizontalAlignment="HorizontalAlignment.Start">
                <FluentLabel Style="width:100px">Item Name</FluentLabel>
                <FluentLabel Style="width:300px" @bind-Value="@Content.ItemName" />
            </FluentStack>
        }
       
 
        <FluentStack Orientation="Orientation.Horizontal" HorizontalAlignment="HorizontalAlignment.Start">
            <FluentLabel Style="width:100px">Other Notes</FluentLabel>
            <FluentTextField Style="width:300px" @bind-Value="@Content.OtherNotes" />
        </FluentStack>

        <FluentStack Orientation="Orientation.Horizontal" HorizontalAlignment="HorizontalAlignment.Start">
            <FluentLabel Style="width:100px">US Teeth</FluentLabel>
            <FluentTextField @onchange="OnUSTeethChange" @bind-Value="@Content.UsTeeth" Style="width:250px" />
            <FluentButton Appearance="Appearance.Outline" IconStart="@(new Icons.Regular.Size20.SubtractCircle())" OnClick="@CleanTeeth"></FluentButton>

        </FluentStack>
        <FluentStack Orientation="Orientation.Horizontal" HorizontalAlignment="HorizontalAlignment.Start">
            <FluentLabel Style="width:100px">Quantity</FluentLabel>
            <FluentNumberField @bind-Value="@Content.Quantity" Style="width:70px" />
        </FluentStack>
        <FluentStack Orientation="Orientation.Horizontal" HorizontalAlignment="HorizontalAlignment.Start">
            <FluentLabel Style="width:100px">Shade</FluentLabel>
            <FluentTextField Style="width:180px" @bind-Value="@Content.Shade" />
        </FluentStack>        
    </FluentStack>
    <FluentStack Orientation="Orientation.Vertical" Style="width:300px">

        <FluentStack Orientation="Orientation.Horizontal" HorizontalAlignment="HorizontalAlignment.Center">
            <FluentRadioGroup Name="numbers" @bind-Value="@SelectType" Label="Type: " @onchange="OnSelectTypeChange">
                <FluentRadio Value="1">Single</FluentRadio>
                <FluentRadio Value="2">Bridge</FluentRadio>
            </FluentRadioGroup>
        </FluentStack>


        <FluentStack Orientation="Orientation.Vertical" HorizontalAlignment="HorizontalAlignment.Start" Style="width:200;height:300">



